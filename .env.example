# Firebase Configuration - NEVER commit real values!
# Copy this file to .env and fill in your actual Firebase credentials
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=1:your_id:web:your_web_id
VITE_FIREBASE_MEASUREMENT_ID=G-YOUR_MEASUREMENT_ID

# Comma-separated admin emails allowed to access /admin
VITE_ADMIN_EMAILS=admin@example.com
